complete <- function(directory, id = 1:332) {
setwd(directory)
q<-data.frame(id=numeric(),nobs=numeric())
for (i in id) {
x <- read.csv(as.character(paste0(sprintf("%03d",i),".csv")))
y <- x[complete.cases(x), ]
count1<-nrow(y)
z<-data.frame(id=i,nobs=count1)
q<-rbind(q,z)
}
return(q)
}
